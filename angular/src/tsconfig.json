{
  "compilerOptions": {
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es6",
      "dom"
    ],
    "mapRoot": "./",
    "module": "es6",
    "skipLibCheck": true,
    "moduleResolution": "node",
    "outDir": "../dist/out-tsc",
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [
      "../node_modules/@types"
    ],
    "baseUrl": ".",
    "paths": {
      "@abp/*": [
        "../node_modules/abp-ng2-module/src/*"
      ],
      "@app/*": [
        "./app/*"
      ],
      "@shared/*": [
        "./shared/*"
      ],
      "@datatypes/*": [
        "./app/main/datatypes/*"
      ],
      "@node_modules/*": [
        "../node_modules/*"
      ],
      "@angular/*": [
        "../node_modules/@angular/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
